# Fla-loader Configuration Example

# Maximum file upload size (in bytes)
# Default: 104857600 (100MB)
FLA_LOADER_MAX_FILE_SIZE=104857600

# Session token expiration (in days)
# Default: 30
FLA_LOADER_TOKEN_EXPIRY_DAYS=30

# Storage path for uploaded files
# Relative to storage/app/
FLA_LOADER_STORAGE_PATH=fla-loader

# Enable debug mode (logs additional information)
# Default: false
FLA_LOADER_DEBUG=false
